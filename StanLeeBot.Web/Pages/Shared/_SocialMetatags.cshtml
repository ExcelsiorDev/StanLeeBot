@{
    const string description = "The StanLeeBot scours the Marvel and DC Comics website to find information that you're looking for. Integrations with Slack and Telegram.";
}

<!-- Search Engine -->
<meta name="description" content="@description" />
<meta name="image" content="https://stanleebot.com/images/StanLee-1024x1024.png">
<!-- Google -->
<meta itemprop="name" content="StanLeeBot" />
<meta itemprop="description" content="@description" />
<meta itemprop="image" content="https://stanleebot.com/images/StanLee-1024x1024.png" />
<script type="application/ld+json">
    {
    "@@context": "https://schema.org",
    "@@type": "Organization",
    "url": "https://stanleebot.com",
    "logo": "https://stanleebot.com/images/StanLee-1024x1024.png"
    }
</script>
<!-- Twitter -->
<meta name="twitter:card" content="summary_large_image" />
<meta name="twitter:title" content="StanLeeBot" />
<meta name="twitter:description" content="@description" />
<meta name="twitter:site" content="@@stanleebot" />
<meta name="twitter:creator" content="@@stanleebot" />
<meta name="twitter:image" content="https://stanleebot.com/images/StanLee-1200x675.png" />
<meta name="twitter:image:alt" content="@description" />
<!-- Open Graph general (Facebook, Pinterest & Google+) -->
<meta property="og:title" content="StanLeeBot" />
<meta property="og:site_name" content="StanLeeBot" />
<meta property="og:type" content="website" />
<meta property="og:description" content="@description" />
<meta property="og:image" content="https://stanleebot.com/images/StanLee-1200x630.png" />
<meta property="og:image:alt" content="@description" />
<meta property="og:image:type" content="image/png" />
<meta property="og:image:width" content="1200" />
<meta property="og:image:height" content="630" />
<meta property="og:url" content="https://stanleebot.com" />
<meta property="fb:admins" content="2046635" />
<meta property="fb:app_id" content="621575301927860" />
<!-- Slack -->
<meta name="slack-app-id" content="ARQNHCE5Q">